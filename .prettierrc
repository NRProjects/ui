{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 4,
	"printWidth": 120,
	"bracketSpacing": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"svelteStrict"
}
